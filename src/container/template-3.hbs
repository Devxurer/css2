			{{!-- HEADER --}}
			<header class="header">
				<div class="">
					{{> logo}}
				</div>
				<nav class="header_nav">
					<div class="header_nav_right">
						
							{{> nav list=header}}
					</div>
					
				</nav>
			</header>
			
			{{!-- HERO CODE --}}
			{{#with hero}}
			<div class="hero">
				<div class="hero_content">
					<h1 class="hero_title">
						{{title}}
					</h1>
					<p class="hero_text">
						{{text}}
					</p>
				</div>
				<img src="{{photo}}" alt="Hero Image" class="hero_image">
			</div>
			{{/with}}

			{{!-- BLOG CODE --}}
			
			<main class="blog">
				{{#each blog}}
					{{> cardBlog data=this}}
				{{/each}}
			</main> 
			

			{{!-- FORM CODE --}}
			
			<div class="form">
				{{#with form}}
				<div class="form_content">
					<div>
						<h3 class="form_title">{{title}}</h3>
						<p class="form_info">{{info}}</p>
					</div>
					<a class="button form_button" href="{{href}}">{{button}}</a>
				</div>
				{{/with}}
			</div>
			
		

			{{!-- FOOTER --}}
			<footer class="row footer">
				<div class="">
					{{> logo}}
					<span class="foter_copyright">Copyright 2023-2024</span>
				</div>
				{{#each footer}}
				<div class="">
					<span class="footer_title">{{title}}</span>
					{{> nav list=links isVertical=true}}
				</div>
				{{/each}}
			</footer>
		
	
